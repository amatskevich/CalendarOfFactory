<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="by.matskevich.calendaroffactory"
    android:versionCode="1"
    android:versionName="1.1" >

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="16" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher2"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="by.matskevich.calendaroffactory.settings.SettingsActivity"
            android:label="@string/settings_title"
            android:parentActivityName="by.matskevich.calendaroffactory.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="by.matskevich.calendaroffactory.MainActivity" />
        </activity>
    </application>

</manifest>